                                                                <img class='projectPic' src='/media/icons/<%=post[0].projectHashtag%>.png'>
                                                                <h3 class='projectTitle'><%=post[0].projectName%></h3>
                                                            </div>
                                                            <div class="spacer"></div>
                                                            <div class="projectNamesContainer">
                                                                <% for (let name of post[0].studentNames) { %>
                                                                    <div class="projectNameContainer">
                                                                        <img class='projectNamesPic' src='/media/studentHeadshots/<%=name.instaName%>.jpg'>
                                                                        <h3 class='projectNames'><%=`${name.firstName} ${name.lastName}`%></h3>
                                                                    </div>
                                                                <% } %> 
                                                            </div>
                                                        </div>
                                                    <% } else { %>
                                                        <% for (let name of post[0].studentNames) { %>
                                                            <div class='profile'>
                                                                <img class='profilePic' src='/media/studentHeadshots/<%=name.instaName%>.jpg'>
                                                                <h3 class='profileName'><%=`${name.firstName} ${name.lastName}`%></h3>
                                                            </div>
                                                        <% } %>
                                                    <% } %> 
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="cardSide cardSideBack card1Back" id='card1Back'>
                                        <% if (post[1].mediaType === 'VIDEO') { %>
                                            <video class='img imgCardBack imgCard1Back' id='imgCard1Back' autoplay loop muted>
                                                <source src=<%=post[1].imgSource%>
                                                        type="video/mp4">
                                                Sorry, your browser doesn't support embedded videos.
                                            </video>
                                        <% } else { %>
                                            <img class='img imgCardBack imgCard1Back' id='imgCard1Back'src='<%=post[1].imgSource%>'>   
                                        <% } %>
                                        <div class="textbox">
                                            <div class="dateAndCaption">
                                                <h3 class='date'><%=post[1].date%></h3>
                                                <p class='caption'><%=post[1].text%></p>
                                            </div>
                                            <div class='profiles'>
                                                <h4 class='sampleText'>Sample prepared and photographed by:</h4>
                                                <div class='profileList'>
                                                    <% if (post[1].projectName != undefined) { %>
                                                        <div class='projectProfile'>
                                                            <div class="projectPicContainer">
                                                                <img class='projectPic' src='/media/icons/<%=post[1].projectHashtag%>.png'>
                                                                <h3 class='projectTitle'><%=post[1].projectName%></h3>
                                                            </div>
                                                            <div class="spacer"></div>
                                                            <div class="projectNamesContainer">
                                                                <% for (let name of post[1].studentNames) { %>
                                                                    <div class="projectNameContainer">
                                                                        <img class='projectNamesPic' src='/media/studentHeadshots/<%=name.instaName%>.jpg'>
                                                                        <h3 class='projectNames'><%=`${name.firstName} ${name.lastName}`%></h3>
                                                                    </div>
                                                                <% } %> 
                                                            </div>
                                                        </div>
                                                    <% } else { %>
                                                        <% for (let name of post[1].studentNames) { %>
                                                            <div class='profile'>
                                                                <img class='profilePic' src='/media/studentHeadshots/<%=name.instaName%>.jpg'>
                                                                <h3 class='profileName'><%=`${name.firstName} ${name.lastName}`%></h3>
                                                            </div>
                                                        <% } %>
                                                    <% } %> 
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    <% } %>   
                    <% counter ++ %>
                <% } %>
                <div class="toggleControls">
                    <div class="controlsSpacer"></div>
                    <button id="toggleForwards">></button>
                    <div class="playPauseBtnContainer">
                        <button class="toggleControls playPauseControls" id="playPauseBtn">Pause</button>
                        <button class="toggleControls playPauseControls" id="fullScreenBtn">
                            <svg id='compressScreenIcon' aria-hidden="true" focusable="false" data-prefix="fas" data-icon="compress" class="svg-inline--fa fa-compress fa-w-14" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"><path fill="currentColor" d="M436 192H312c-13.3 0-24-10.7-24-24V44c0-6.6 5.4-12 12-12h40c6.6 0 12 5.4 12 12v84h84c6.6 0 12 5.4 12 12v40c0 6.6-5.4 12-12 12zm-276-24V44c0-6.6-5.4-12-12-12h-40c-6.6 0-12 5.4-12 12v84H12c-6.6 0-12 5.4-12 12v40c0 6.6 5.4 12 12 12h124c13.3 0 24-10.7 24-24zm0 300V344c0-13.3-10.7-24-24-24H12c-6.6 0-12 5.4-12 12v40c0 6.6 5.4 12 12 12h84v84c0 6.6 5.4 12 12 12h40c6.6 0 12-5.4 12-12zm192 0v-84h84c6.6 0 12-5.4 12-12v-40c0-6.6-5.4-12-12-12H312c-13.3 0-24 10.7-24 24v124c0 6.6 5.4 12 12 12h40c6.6 0 12-5.4 12-12z"></path></svg>
                            <svg id='fullScreenIcon' aria-hidden="true" focusable="false" data-prefix="fas" data-icon="expand" class="svg-inline--fa fa-expand fa-w-14" role="img" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 448 512"><path fill="currentColor" d="M0 180V56c0-13.3 10.7-24 24-24h124c6.6 0 12 5.4 12 12v40c0 6.6-5.4 12-12 12H64v84c0 6.6-5.4 12-12 12H12c-6.6 0-12-5.4-12-12zM288 44v40c0 6.6 5.4 12 12 12h84v84c0 6.6 5.4 12 12 12h40c6.6 0 12-5.4 12-12V56c0-13.3-10.7-24-24-24H300c-6.6 0-12 5.4-12 12zm148 276h-40c-6.6 0-12 5.4-12 12v84h-84c-6.6 0-12 5.4-12 12v40c0 6.6 5.4 12 12 12h124c13.3 0 24-10.7 24-24V332c0-6.6-5.4-12-12-12zM160 468v-40c0-6.6-5.4-12-12-12H64v-84c0-6.6-5.4-12-12-12H12c-6.6 0-12 5.4-12 12v124c0 13.3 10.7 24 24 24h124c6.6 0 12-5.4 12-12z"></path></svg>
                        </button>
                    </div>
                </div> 
            </div>
            <div class="toggleControlsPhone">
                <div class="togglePhoneArrows" id="togglePhoneBackwards"><</div>
                <div class="togglePhoneArrows" id="togglePhonePlayPause"><i class="material-icons" id="pauseSymbol">pause</i><i class="material-icons" id="playSymbol">play_arrow</i></div>
                <div class="togglePhoneArrows" id="togglePhoneForwards">></div>
            </div> 
            <%- include('../partials/footer')%>
        </main>
         
    <script src="/javaScripts/flipCards.js"></script>
</body>
</html>